@model CarpoolingCR.Objects.Responses.CancelNotificationResponse

@{
    ViewBag.Title = "Index";
}
<div id="notification-request-index-container" class="row container">
    @Html.Partial("~/Views/NotificationRequests/Partials/p_Index.cshtml", Model)
</div>

<div class="modal fade" id="cancelNotRequestModal" tabindex="-1" role="dialog" aria-labelledby="cancelNotRequestModalLabel" aria-hidden="true">
    <div class="vertical-alignment-helper">
        <div class="modal-dialog vertical-align-center">
            <div class="modal-content">
                <div class="modal-header">
                    <input type="text" id="selected-notification-id" name="selected-notification-id" class="hidden" />
                    <input type="text" id="selected-user-id" name="selected-user-id" class="hidden" />
                    <button type="button" class="close" data-dismiss="modal">
                        <span aria-hidden="true">&times;</span><span class="sr-only">Close</span>
                    </button>
                    <h4 class="modal-title" id="dayTripModalLabel">Cancelación de notificaciones</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <small>¡Por favor confirme que desea cancelar esta notificación!</small>
                        </div>
                    </div>
                </div>
                <div id="btnsContainer" class="modal-footer">
                    <input type="button" class="btn btn-default btn-sm" style="width:150px" data-dismiss="modal" value="Cerrar" />
                    <input type="button" class="btn btn-danger btn-sm" style="width:150px" onclick="CancelNotification()" value="Cancelar" />
                </div>
            </div>
        </div>
    </div>
</div>

