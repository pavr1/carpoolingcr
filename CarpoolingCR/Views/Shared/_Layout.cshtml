                    animation-fill-mode: both;
                }

                .smallseat input[type=checkbox]:disabled + label {
                    background: #dddddd;
                    text-indent: -9999px;
                    overflow: hidden;
                }

                    .smallseat input[type=checkbox]:disabled + label:after {
                        content: "X";
                        text-indent: 0;
                        position: absolute;
                        top: 5px;
                        left: 50%;
                        transform: translate(-50%, 0%);
                    }

                    .smallseat input[type=checkbox]:disabled + label:hover {
                        box-shadow: none;
                        cursor: not-allowed;
                    }

            .smallseat label {
                display: block;
                position: relative;
                width: 100%;
                text-align: center;
                font-size: 14px;
                font-weight: bold;
