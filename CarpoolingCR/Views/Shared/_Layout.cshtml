    <script>
        $('.money').click(function () {
            $(this).select();
        });

        $('.money').change(function () {
            $(this).select();
            var moneyValue = $(this).val();

            var formatter = new Intl.NumberFormat('es-CR', {
                style: 'currency',
                currency: 'CRC',
            });

            $(this).val(formatter.format(moneyValue));
        });

        $('.emailvalidation').change(function () {
            var email = $(this).val();

            $('#Email').val(email);

            if (!validateEmail(email)) {
                $('#Email').css("background", "#FADBD8");
            } else {
                $('#Email').css("background", "#D1F2EB");
            }
        });

        function PopUpQualifyModal(reservationId, tripId, userName, url) {
            $('#q-reservation-id').val(reservationId);
            $('#q-trip-id').val(tripId);
            $('#p-User-Name').html(userName);
            $('#q-comment').val('');
            $('#q-star4').removeClass('checked');
            $('#q-star5').removeClass('checked');

            $('#qualifyUserModal').modal();
        }

        function QualifyUser() {
            var reservationId = $('#q-reservation-id').val();
            var tripId = $('#q-trip-id').val();
            var stars = $('#q-stars').val();
            var comment = $('#q-comment').val();

            if (tripId == '') {
                tripId = null;
            }

            console.log('ReservationId: ' + reservationId + ', TripId: ' + tripId + ', Stars: ' + stars + ', Comment: ' + comment);

            $.post('@Url.Action("QualifyUser", "Qualifications")', { reservationId: reservationId, tripId: tripId, stars: stars, comment: comment},
                function (data) {
                    $('#qualifyUserModal').modal('toggle');

                    if (data != '') {
                        var json = JSON.parse(data);

                        sleep(400);

                        if (json.Message != '') {
                            DisplayGrowlMessage(json.Message, json.MessageType);
                        }

                        $('#container-id').html(json.Html);
                    } else {
                        DisplayGrowlMessage('¡Error inesperado, intente de nuevo!', 'error');
                    }
                });
        }

        function SetStars(stars, prefix) {
            console.log("Setting stars...");

            $('#' + prefix + 'star1').removeClass('checked');
            $('#' + prefix + 'star2').removeClass('checked');
            $('#' + prefix + 'star3').removeClass('checked');
            $('#' + prefix + 'star4').removeClass('checked');
            $('#' + prefix + 'star5').removeClass('checked');

            $('#q-stars').val(stars);

            for (var i = 1; i <= stars; i++) {
                $('#' + prefix + 'star' + i).addClass('checked');
            }
        }

        $(document).ready(function () {
            checkMessages();

            //default
            $('#q-stars').val(3);
        });

        function checkMessages() {
            var error = '@ViewBag.Error';
            var warning = '@ViewBag.Warning';
            var info = '@ViewBag.Info';

            if (error != '') {
                DisplayGrowlMessage(error, 'error');
            }

            if (warning != '') {
                DisplayGrowlMessage(warning, 'warn');
            }

            if (info != '') {
                DisplayGrowlMessage(info, 'success');
            }
        }

        function displayMessages(message, type) {
            if (type == 'error') {
                DisplayGrowlMessage(message, 'error');
            }

            if (type == 'warning') {
                DisplayGrowlMessage(message, 'warn');
            }

            if (type == 'info') {
                DisplayGrowlMessage(message, 'success');
            }
        }

        function ScrollUp() {
            $('html, body').animate({
                scrollTop: 0
            }, 1000);
        }

        function sleep(milliseconds) {
            var start = new Date().getTime();
            for (var i = 0; i < 1e7; i++) {
                if ((new Date().getTime() - start) > milliseconds) {
                    break;
                }
            }
        }

        function DisplayGrowlMessage(message, type) {
            var codingmessage = GetMessage(message);

            if (codingmessage === "") {
                $.notify(message, type);
            } else {
                $.notify(codingmessage, type);
            }
        }

        function SetRandomImage() {
            var types = ['abstract', 'city', 'people', 'transport', 'animals', 'food', 'nature', 'business', 'nightlife', 'sports', 'cats', 'fashion', 'technics'];
            var baseUrl = 'http://lorempixel.com/100/100/'
            var selectedTypeIndex = GetRandomNumber(14);
            var selectedType = types[selectedTypeIndex];

            var selectedPic = GetRandomNumber(11);

            baseUrl += selectedType + "/" + selectedPic + "/"

            return baseUrl;
        }

        function GetRandomNumber(top) {
            return Math.floor(Math.random() * top)
        }

        //$('.btnLink').click(function () {
        //    $(this).html('<span class="spinner-border spinner-border-sm mr-2" role="status" aria-hidden="true"></span>Procesando...').addClass('disabled');
        //});

        $('.custom-spin-button').on('click', function () {
            var $this = $(this);
            $this.button('loading');
        });
    </script>

